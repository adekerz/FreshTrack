{
  "name": "freshtrack-server",
  "version": "2.0.0",
  "description": "FreshTrack Backend Server - PostgreSQL Edition",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "migrate": "node db/migrate.js",
    "migrate:status": "node db/migrate.js --status",
    "migrate:rollback": "node db/migrate.js --rollback",
    "backup": "node db/backup.js",
    "backup:gzip": "node db/backup.js --gzip",
    "backup:schema": "node db/backup.js --format=schema-only",
    "backup:data": "node db/backup.js --format=data-only"
  },
  "dependencies": {
    "@sentry/node": "^10.32.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dejavu-fonts-ttf": "^2.37.3",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "otplib": "^12.0.1",
    "pdfkit": "^0.15.2",
    "pg": "^8.11.3",
    "psql": "^0.0.1",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^9.0.1",
    "resend": "^6.8.0",
    "svix": "^1.40.0",
    "ua-parser-js": "^1.0.41",
    "uuid": "^9.0.0",
    "zod": "^4.3.4"
  },
  "overrides": {
    "qs": ">=6.14.1"
  },
  "keywords": [
    "freshtrack",
    "inventory",
    "postgresql",
    "hotel"
  ],
  "author": "Adilet Y.",
  "license": "ISC"
}
