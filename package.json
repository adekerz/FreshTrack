{
  "name": "freshtrack",
  "version": "1.0.0",
  "description": "Enterprise Inventory & Expiration Date Management System",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cd server && npm install",
    "start": "cd server && node index.js",
    "dev:server": "cd server && node --watch index.js",
    "dev:client": "vite",
    "build:client": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:a11y": "eslint src --ext js,jsx --plugin jsx-a11y",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:server": "vitest run --config vitest.server.config.ts",
    "test:all": "vitest run && vitest run --config vitest.server.config.ts",
    "lighthouse": "lighthouse http://localhost:5173 --output html --output-path ./reports/lighthouse.html --chrome-flags=\"--headless\"",
    "lighthouse:ci": "lighthouse http://localhost:5173 --output json --output-path ./reports/lighthouse.json --budget-path=./lighthouse-budget.json",
    "generate-apple-touch-icon": "node scripts/generate-apple-touch-icon.mjs"
  },
  "dependencies": {
    "@sentry/node": "^10.32.1",
    "@tanstack/query-sync-storage-persister": "^5.90.21",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-query-persist-client": "^5.90.21",
    "@vercel/speed-insights": "^1.3.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "nodemailer": "^7.0.12",
    "pdfmake": "^0.2.20",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "resend": "^6.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.4.0",
    "lighthouse": "^11.4.0",
    "postcss": "^8.4.31",
    "prettier": "^3.7.4",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.3.5",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.16"
  }
}
